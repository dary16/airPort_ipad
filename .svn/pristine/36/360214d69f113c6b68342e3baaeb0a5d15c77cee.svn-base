<!DOCTYPE html>
<html class="no-js css-menubar" lang="zh-cn">

    <head>
        <title>拦阻网维护</title>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <!-- 移动设备 viewport -->
        <meta name="viewport" content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no,minimal-ui" />
        <!-- 第三方库 -->
        <script type="text/javascript" src="./lib/jquery-1.8.3.min.js"></script>
        <script type="text/javascript" src="./lib/layui/layui.all.js"></script>
        <link rel="stylesheet" href="./lib/layui/css/layui.css" />
        <script type="text/javascript" src="./lib/zTree/js/jquery.ztree.all.min.js"></script>
        <link href="./lib/zTree/css/zTreeStyle.css" rel="stylesheet" />
        <!-- 公共js -->
        <script type="text/javascript" src="./js/common.js"></script>
        <!-- 当前页需要的js -->
        <script type="text/javascript" src="./js/list-vehicle-management-edit.js"></script>
        <!-- 字体 -->
        <link href="css/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
        <!-- 公共样式 -->
        <link href="./css/style.css" rel="stylesheet" />
        <link href="./css/list-edit.css" rel="stylesheet" />
    </head>

    <body>
        <div class="scheduling">
            <div class="header">
                <a id="comBack" class="back" href="javascript:;"> <img src="img/back.png" /> </a>
                <p>新增</p>
            </div>
            <div class="contentEdit">
                <form id="form" class="layui-form clearfix">
                    <div class="searchItem">
                        <label class="title">汽车类型：</label>
                        <div class="inputBlock">
                            <select name="carType">
                                <option value="">请选择类型</option>
                                <option value="指挥车">指挥车</option>
                                <option value="轿车">轿车</option>
                                <option value="旅行车">旅行车</option>
                                <option value="大客车">大客车</option>
                                <option value="运输车">运输车</option>
                                <option value="吊车">吊车</option>
                                <option value="自卸车">自卸车</option>
                                <option value="大牵引车">大牵引车</option>
                                <option value="牵引车">牵引车</option>
                                <option value="加油车">加油车</option>
                                <option value="抽油车">抽油车</option>
                            </select>
                        </div>
                    </div>
                    <div class="searchItem">
                        <label class="title">车牌号：</label>
                        <div class="inputBlock"><input class="layui-input" type="text" name="carCode" placeholder="请输入车牌号" autocomplete="off" /></div>
                    </div>
                    <div class="searchItem">
                        <label class="title">型号：</label>
                        <div class="inputBlock"><input class="layui-input" type="text" name="model" placeholder="请输入型号" autocomplete="off" /></div>
                    </div>
                    <div class="searchItem">
                        <label class="title">底盘号：</label>
                        <div class="inputBlock"><input class="layui-input" type="text" name="chassisNum" placeholder="请输入底盘号" autocomplete="off" /></div>
                    </div>
                    <div class="searchItem">
                        <label class="title">发动机号：</label>
                        <div class="inputBlock"><input class="layui-input" type="text" name="engineNum" placeholder="请输入发动机号" autocomplete="off" /></div>
                    </div>
                    <div class="searchItem">
                        <label class="title">出厂日期：</label>
                        <div class="inputBlock"><input class="layui-input" id="deliveryTime" type="text" name="deliveryTime" placeholder="请选择出厂日期" /></div>
                    </div>
                    <div class="searchItem">
                        <label class="title">配发日期：</label>
                        <div class="inputBlock"><input class="layui-input" id="withTime" type="text" name="withTime" placeholder="请选择配发日期" /></div>
                    </div>
                    <div class="searchItem">
                        <label class="title">生产厂家：</label>
                        <div class="inputBlock"><input class="layui-input" type="text" name="manufacturer" placeholder="请输入生产厂家" autocomplete="off" /></div>
                    </div>
                    <div class="searchItem">
                        <label class="title">质量状况：</label>
                        <div class="inputBlock"><input class="layui-input" type="text" name="quality" placeholder="请输入质量状况" autocomplete="off" /></div>
                    </div>
                    <div class="searchItem"><button type="button" class="searchBtn">完成</button></div>
                </form>
            </div>
        </div>
    </body>

</html>