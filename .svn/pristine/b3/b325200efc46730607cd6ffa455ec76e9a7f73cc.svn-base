.leaflet-popup-tip,
.leaflet-popup-content-wrapper {
    background-color: rgb(0, 25, 46);
    color: #3fc9d4;
}

#interGroup {
    width: 200px;
    height: 300px;
    position: absolute;
    top: 75px;
    left: 0px;
    z-index: 9999;
    background: #f1eded;
    overflow-y: auto;
}

.orgGroup {
    width: 200px;
    height: 300px;
    position: absolute;
    top: 395px;
    left: 0px;
    z-index: 9999;
    background: #f1eded;
    overflow-y: auto;
}

.addressGroup {
    width: 200px;
    height: 300px;
    position: absolute;
    top: 715px;
    left: 0px;
    z-index: 9999;
    background: #f1eded;
    overflow-y: auto;
}

#weatherList {
    width: 600px;
    height: 400px;
    position: absolute;
    top: 100px;
    left: 300px;
    z-index: 9999;
    background: #f1eded;
    overflow-y: auto;
}

#weatherList ul {
    padding: 10px 20px;
    overflow: hidden;
}

#weatherList ul li {
    list-style: none;
    float: left;
    margin-right: 10px;
}

.layui-form-select dl {
    background-color: #919297 !important;
}

.querybar-ssk {
    left: 0 !important;
}

#checkbar {
    position: absolute;
    top: 100px;
    left: 20px;
    width: 600px;
    height: 200px;
    z-index: 9999;
    background: #fff;
    padding-top: 20px;
}

/* .layui-form-checked span,
.layui-form-checked:hover span {
    background-color: #20a0ff !important;
} */


.toolbar {
    position: absolute;
    right: 0.17rem;
    top: 1.5rem;
    width: 7.24rem;
    height: 0.71rem;
    background: #000;
    border-radius: 0.05rem;
    z-index: 9999;
}

.toolbar ul {
    height: 0.71rem;
    line-height: 0.71rem;
}

.toolbar li {
    font-size: 0.3rem;
    color: #fff;
    display: inline-block;
    width: 24.3%;
    text-align: center;
    border-right: 1px solid #575757;
}

.toolbar li:last-child {
    border-right: none;
}

.toolbar li img {
    width: 0.4rem;
    height: 0.4rem;
}

.layui-layer-molv .layui-layer-title {
    color: #3f3f3f !important;
    background-color: #dcdcdc !important;
    padding: 0 .3rem 0 .2rem !important;
    border-top-left-radius: .16rem;
    border-top-right-radius: .16rem;
}

.layui-layer-molv {
    border-top-left-radius: .16rem !important;
    border-top-right-radius: .16rem !important;
}

.itemLeft {
    float: left;
    width: 33%;
    height: .9rem;
    line-height: .9rem;
}

#form {
    padding-left: .5rem;
    padding-right: .3rem;
    background: #eee;
    height: .9rem;
    line-height: .9rem
}

.itemLeft .title {
    font-size: .34rem;
    color: #3f3f3f;
    height: .9rem;
    line-height: .9rem;
    margin-bottom: 0 !important;
    font-weight: normal;
}

.itemRight {
    float: right;
}

.layui-form-switch i {
    width: .4rem !important;
    height: .4rem !important;
}

.layui-form-switch {
    width: 1rem !important;
    height: .5rem !important;
    margin-top: 0 !important;
}

.layui-form-onswitch i {
    margin-left: -24px !important;
    top: 2px !important;
}

.layui-form-onswitch {
    border-color: #339ed4 !important;
    background-color: #339ed4 !important;
    width: 1rem;
    height: .5rem;
}

.layui-layer-page .layui-layer-content {
    background: #eee;
}

/* .layui-form-checkbox[lay-skin=primary] span {
    font-size: .34rem !important;
} */

.layui-form-item .layui-form-checkbox[lay-skin=primary] {
    margin-top: 14px !important;
}


.layui-form-checked[lay-skin=primary] i {
    background-color: #eee !important;
    color: #09bb07 !important;
    border-color: #bfbfbf !important;
}

.nearby {
    position: absolute;
    right: 0.17rem;
    top: 2.5rem;
    width: 5rem;
    height: 7rem;
    background: #fff;
    border-radius: 0.05rem;
    z-index: 9999;
    color: #333;
    padding: 10px;
}

.nearby li {
    border-bottom: 1px solid #333;
}

.nearby li span {
    display: inline-block;
    width: 50%;
}