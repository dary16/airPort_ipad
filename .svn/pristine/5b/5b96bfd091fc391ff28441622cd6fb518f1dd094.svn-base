<!DOCTYPE html>
<html class="no-js css-menubar" lang="zh-cn">

    <head>
        <title>鸟情通报</title>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <!-- 移动设备 viewport -->
        <meta name="viewport" content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no,minimal-ui" />
        <!-- 第三方库 -->
        <script type="text/javascript" src="./lib/jquery-1.8.3.min.js"></script>
        <script type="text/javascript" src="./lib/layui/layui.all.js"></script>
        <link rel="stylesheet" href="./lib/layui/css/layui.css" />
        <script type="text/javascript" src="./lib/zTree/js/jquery.ztree.all.min.js"></script>
        <link href="./lib/zTree/css/zTreeStyle.css" rel="stylesheet" />
        <!-- 公共js -->
        <script type="text/javascript" src="./js/common.js"></script>
        <!-- 当前页需要的js -->
        <script type="text/javascript" src="./js/list-bird-notice-edit.js"></script>
        <!-- 字体 -->
        <link href="css/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
        <!-- 公共样式 -->
        <link href="./css/style.css" rel="stylesheet" />
        <link href="./css/list-edit.css" rel="stylesheet" />
    </head>

    <body>
        <div class="scheduling">
            <div class="header">
                <a id="comBack" class="back" href="javascript:;"> <img src="img/back.png" /> </a>
                <p>新增</p>
            </div>
            <div class="contentEdit">
                <form id="form" class="layui-form clearfix">
                    <div class="searchItem">
                        <label class="title">标题：</label>
                        <div class="inputBlock"><input class="layui-input" type="text" name="title" placeholder="请输入标题" autocomplete="off" /></div>
                    </div>
                    <div class="searchItem">
                        <label class="title">时间：</label>
                        <div class="inputBlock"><input class="layui-input" id="sendTime" type="text" name="nowTime" placeholder="请选择时间" autocomplete="off" /></div>
                    </div>
                    <div class="searchItem">
                        <label class="title">上报人：</label>
                        <div class="inputBlock"><input class="layui-input" type="text" name="userName" placeholder="请输入上报人" /></div>
                    </div>
                    <div class="searchItem w100">
                        <label class="title">内容：</label>
                        <div class="inputBlock">
                            <textarea name="content" placeholder="请输入内容" class="layui-textarea"></textarea>
                        </div>
                    </div>

                    <div class="searchItem"><button type="button" class="searchBtn">完成</button></div>
                </form>
            </div>
        </div>
    </body>

</html>